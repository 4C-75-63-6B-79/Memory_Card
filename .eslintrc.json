{
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "ignorePatterns": ["./public"],
    "rules": {
        "no-console": "off",
        "quotes": [
            "error",
            "double"
        ],
        "semi": ["error", "always"],
        "object-curly-spacing": ["error", "always"]
    }
}